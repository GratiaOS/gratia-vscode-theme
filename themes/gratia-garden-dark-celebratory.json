{
  "$schema": "vscode://schemas/color-theme",
  "name": "Gratia â€” Garden Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#001A35",
    "editor.foreground": "#EEF7F5",
    "editorCursor.foreground": "#3ED0C8",

    "editor.lineHighlightBackground": "#001730",
    "editor.lineHighlightBorder": "#00162D",

    "editor.selectionBackground": "#00405D",
    "editor.selectionForeground": "#EEF7F5",
    "editor.inactiveSelectionBackground": "#3ED0C84D",
    "editor.selectionHighlightBackground": "#3ED0C84D",
    "editor.wordHighlightBackground": "#3ED0C84D",
    "editor.wordHighlightStrongBackground": "#3ED0C88C",

    "editorBracketMatch.border": "#3ED0C866",

    "editorLineNumber.foreground": "#3C5165",
    "editorGutter.addedBackground": "#3ED0C866",
    "editorGutter.modifiedBackground": "#3ED0C866",
    "editorGutter.deletedBackground": "#3ED0C866",

    "editorGroupHeader.tabsBackground": "#001A35",
    "editorGroupHeader.tabsBorder": "#001A35",
    "tab.activeBackground": "#003653",
    "tab.inactiveBackground": "#001A35",
    "tab.border": "#00000000",
    "tab.activeBorder": "#1CA8B1",
    "tab.unfocusedActiveBorder": "#1CA8B173",
    "tab.activeForeground": "#EEF7F5",
    "tab.inactiveForeground": "#9BAAB2",

    "activityBar.background": "#001428",
    "activityBar.foreground": "#EEF7F5",
    "activityBarBadge.background": "#3ED0C8",
    "activityBarBadge.foreground": "#001A35",

    "sideBar.background": "#001428",
    "sideBar.foreground": "#EEF7F5",
    "sideBarSectionHeader.background": "#00152B",
    "sideBarSectionHeader.foreground": "#EEF7F5",

    "statusBar.background": "#001428",
    "statusBar.foreground": "#EEF7F5",

    "titleBar.activeBackground": "#001428",
    "titleBar.activeForeground": "#EEF7F5",

    "panel.background": "#001A35",
    "panel.border": "#00162D",

    "badge.background": "#00152B",
    "badge.foreground": "#EEF7F5",

    "button.background": "#00152B",
    "button.foreground": "#EEF7F5",
    "button.hoverBackground": "#00162D",

    "focusBorder": "#1CA8B1",

    "input.background": "#06263F",
    "input.foreground": "#EEF7F5",
    "input.border": "#1CA8B1",
    "input.placeholderForeground": "#83949F",
    "inputOption.activeBorder": "#1CA8B199",
    "inputOption.activeBackground": "#0A2A42",

    "selection.background": "#3ED0C88C",
    "list.activeSelectionBackground": "#00405D",
    "list.activeSelectionForeground": "#EEF7F5",
    "list.hoverBackground": "#0A2A42",
    "list.focusBackground": "#00405D",
    "list.focusForeground": "#EEF7F5",
    "list.inactiveSelectionBackground": "#06263F",
    "list.inactiveSelectionForeground": "#9BAAB2",
    "list.dropBackground": "#3ED0C84D",

    "editorWidget.background": "#06263F",
    "editorWidget.border": "#EEF7F50F",
    "quickInput.background": "#003653",
    "quickInputTitle.background": "#003653",
    "quickInputList.focusBackground": "#00405D",
    "quickInputList.focusForeground": "#EEF7F5",
    "dropdown.background": "#06263F",

    "peekView.border": "#EEF7F50F",
    "peekViewEditor.background": "#003653",
    "peekViewResult.background": "#001A35",
    "peekViewTitle.background": "#003653",

    "terminal.background": "#003653",
    "terminal.foreground": "#EEF7F5",
    "terminalCursor.background": "#003653",
    "terminalCursor.foreground": "#3ED0C8",

    "terminal.ansiBlack": "#9DB6C0",
    "terminal.ansiRed": "#C68E89",
    "terminal.ansiGreen": "#84A784",
    "terminal.ansiYellow": "#A59D6F",
    "terminal.ansiBlue": "#819EC2",
    "terminal.ansiMagenta": "#B291BA",
    "terminal.ansiCyan": "#6BA8AB",
    "terminal.ansiWhite": "#235670",
    "terminal.ansiBrightBlack": "#7097A8",
    "terminal.ansiBrightRed": "#FFAFA8",
    "terminal.ansiBrightGreen": "#9DDA9F",
    "terminal.ansiBrightYellow": "#D6CA7B",
    "terminal.ansiBrightBlue": "#9BCBFF",
    "terminal.ansiBrightMagenta": "#F0B5FD",
    "terminal.ansiBrightCyan": "#6EDCE2",
    "terminal.ansiBrightWhite": "#0B4867"
  },
    "tokenColors": [
    { "scope": "comment", "settings": { "foreground": "#6B7D86", "fontStyle": "italic" } },
    { "scope": ["string", "constant.other.symbol"], "settings": { "foreground": "#A7F3D0" } },
    { "scope": ["keyword", "storage.type", "storage.modifier"], "settings": { "foreground": "#C084FC" } },
    { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#93C5FD" } },
    { "scope": ["variable", "meta.definition.variable", "support.variable"], "settings": { "foreground": "#FBBF24" } },
    { "scope": ["entity.name.type", "support.type", "storage.type.cs"], "settings": { "foreground": "#7DD3FC" } },
    { "scope": ["punctuation", "meta.brace", "meta.delimiter"], "settings": { "foreground": "#C9D6DF" } },
    { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#001A35" } }
  ],
  "semanticTokenColors": {
    "variable.readonly.defaultLibrary": "#FBBF24",
    "property": "#EBD38C",
    "parameter": "#EBD38C",
    "function": "#93C5FD",
    "method": "#93C5FD",
    "class": "#7DD3FC",
    "type": "#7DD3FC",
    "enumMember": "#FBBF24",
    "namespace": "#C084FC",
    "string": "#A7F3D0",
    "number": "#FCD34D",
    "boolean": "#F78DBF",
    "keyword": "#C084FC"
  }
}
