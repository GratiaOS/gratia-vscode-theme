{
  "$schema": "vscode://schemas/color-theme",
  "name": "Gratia â€” Garden Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#0B140D",
    "editor.foreground": "#F2EEE3",
    "editorCursor.foreground": "#A98456",

    "editor.lineHighlightBackground": "#0A120C",
    "editor.lineHighlightBorder": "#09110B",

    "editor.selectionBackground": "#252F1D",
    "editor.selectionForeground": "#F2EEE3",
    "editor.inactiveSelectionBackground": "#A984564D",
    "editor.selectionHighlightBackground": "#A984564D",
    "editor.wordHighlightBackground": "#A984564D",
    "editor.wordHighlightStrongBackground": "#A984568C",

    "editorBracketMatch.border": "#A9845666",

    "editorLineNumber.foreground": "#454B43",
    "editorGutter.addedBackground": "#A9845666",
    "editorGutter.modifiedBackground": "#A9845666",
    "editorGutter.deletedBackground": "#A9845666",

    "editorGroupHeader.tabsBackground": "#0B140D",
    "editorGroupHeader.tabsBorder": "#0B140D",
    "tab.activeBackground": "#1E2819",
    "tab.inactiveBackground": "#0B140D",
    "tab.border": "#00000000",
    "tab.activeBorder": "#816E42",
    "tab.unfocusedActiveBorder": "#816E4273",
    "tab.activeForeground": "#F2EEE3",
    "tab.inactiveForeground": "#A1A298",

    "activityBar.background": "#080F0A",
    "activityBar.foreground": "#F2EEE3",
    "activityBarBadge.background": "#A98456",
    "activityBarBadge.foreground": "#0B140D",

    "sideBar.background": "#080F0A",
    "sideBar.foreground": "#F2EEE3",
    "sideBarSectionHeader.background": "#09100B",
    "sideBarSectionHeader.foreground": "#F2EEE3",

    "statusBar.background": "#080F0A",
    "statusBar.foreground": "#F2EEE3",

    "titleBar.activeBackground": "#080F0A",
    "titleBar.activeForeground": "#F2EEE3",

    "panel.background": "#0B140D",
    "panel.border": "#09110B",

    "badge.background": "#09100B",
    "badge.foreground": "#F2EEE3",

    "button.background": "#09100B",
    "button.foreground": "#F2EEE3",
    "button.hoverBackground": "#09110B",

    "focusBorder": "#816E42",

    "input.background": "#161E17",
    "input.foreground": "#F2EEE3",
    "input.border": "#816E42",
    "input.placeholderForeground": "#8A8C83",
    "inputOption.activeBorder": "#816E4299",
    "inputOption.activeBackground": "#1A221A",

    "selection.background": "#A984568C",
    "list.activeSelectionBackground": "#252F1D",
    "list.activeSelectionForeground": "#F2EEE3",
    "list.hoverBackground": "#1A221A",
    "list.focusBackground": "#252F1D",
    "list.focusForeground": "#F2EEE3",
    "list.inactiveSelectionBackground": "#161E17",
    "list.inactiveSelectionForeground": "#A1A298",
    "list.dropBackground": "#A984564D",

    "editorWidget.background": "#161E17",
    "editorWidget.border": "#F2EEE30F",
    "quickInput.background": "#1E2819",
    "quickInputTitle.background": "#1E2819",
    "quickInputList.focusBackground": "#252F1D",
    "quickInputList.focusForeground": "#F2EEE3",
    "dropdown.background": "#161E17",

    "peekView.border": "#F2EEE30F",
    "peekViewEditor.background": "#1E2819",
    "peekViewResult.background": "#0B140D",
    "peekViewTitle.background": "#1E2819",

    "terminal.background": "#1E2819",
    "terminal.foreground": "#F2EEE3",
    "terminalCursor.background": "#1E2819",
    "terminalCursor.foreground": "#A98456",

    "terminal.ansiBlack": "#ABAC9E",
    "terminal.ansiRed": "#C68E89",
    "terminal.ansiGreen": "#84A784",
    "terminal.ansiYellow": "#A59D6F",
    "terminal.ansiBlue": "#819EC2",
    "terminal.ansiMagenta": "#B291BA",
    "terminal.ansiCyan": "#6BA8AB",
    "terminal.ansiWhite": "#404939",
    "terminal.ansiBrightBlack": "#868A7B",
    "terminal.ansiBrightRed": "#FFAFA8",
    "terminal.ansiBrightGreen": "#9DDA9F",
    "terminal.ansiBrightYellow": "#D6CA7B",
    "terminal.ansiBrightBlue": "#9BCBFF",
    "terminal.ansiBrightMagenta": "#F0B5FD",
    "terminal.ansiBrightCyan": "#6EDCE2",
    "terminal.ansiBrightWhite": "#303A29"
  },
    "tokenColors": [
    { "scope": "comment", "settings": { "foreground": "#6B7D86", "fontStyle": "italic" } },
    { "scope": ["string", "constant.other.symbol"], "settings": { "foreground": "#A7F3D0" } },
    { "scope": ["keyword", "storage.type", "storage.modifier"], "settings": { "foreground": "#C084FC" } },
    { "scope": ["entity.name.function", "support.function"], "settings": { "foreground": "#93C5FD" } },
    { "scope": ["variable", "meta.definition.variable", "support.variable"], "settings": { "foreground": "#FBBF24" } },
    { "scope": ["entity.name.type", "support.type", "storage.type.cs"], "settings": { "foreground": "#7DD3FC" } },
    { "scope": ["punctuation", "meta.brace", "meta.delimiter"], "settings": { "foreground": "#C9D6DF" } },
    { "scope": ["invalid", "invalid.deprecated"], "settings": { "foreground": "#0B140D" } }
  ],
  "semanticTokenColors": {
    "variable.readonly.defaultLibrary": "#FBBF24",
    "property": "#EBD38C",
    "parameter": "#EBD38C",
    "function": "#93C5FD",
    "method": "#93C5FD",
    "class": "#7DD3FC",
    "type": "#7DD3FC",
    "enumMember": "#FBBF24",
    "namespace": "#C084FC",
    "string": "#A7F3D0",
    "number": "#FCD34D",
    "boolean": "#F78DBF",
    "keyword": "#C084FC"
  }
}
